#SHELL=/bin/bash
PATH=/sbin:/bin:/usr/sbin:/usr/bin:/var/www/html/CarTracker/php
#MAILTO=root
#HOME=/

#Daily, Weekly, Monthly scripts run at 6:00
0 6 * * * cd /var/www/html/CarTracker/php; php -q updateDaily.php >/dev/null
0 6 * * 1 cd /var/www/html/CarTracker/php; php -q updateWeekly.php >/dev/null
0 6 1 * * cd /var/www/html/CarTracker/php; php -q updateMonthly.php >/dev/null

